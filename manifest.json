{
    "manifest_version": 3,
    "name": "Tabs Copier",
    "version": "1.2",
    "homepage_url": "https://github.com/TheKiromen/TabsCopierExtension",
    "description": "This extension lets you copy links of multiple open tabs to clipboard at once, or open multiple tabs from links stored in clipboard.",
    "author": "dkrucze@gmail.com",
  
    "icons": {
      "64": "icons/icon.png"
    },
  
    "permissions": ["tabs", "clipboardWrite", "clipboardRead", "contextMenus", "menus"],

    "background": {
      "scripts": ["copier.js"]
    },

    "browser_specific_settings": {
      "gecko": {
        "id": "Wouldn't you like to know"
      }
    }
}
  